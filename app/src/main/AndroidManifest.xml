<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.healthboss2">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/icone_icone"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/icone_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Login"></activity>
        <activity android:name=".MarcarConsulta"></activity>
        <activity android:name=".Cadastro" />
        <activity android:name=".Consulta">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".mainactivity.MainActivity"
            android:theme="@style/AppTheme.NoActionBar"></activity>
    </application>

</manifest>